{
	"expo": {
		"name": "Cinemore",
		"slug": "rn_movie_app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/cinemore-app-icon.png",
		"scheme": "movies",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "com.cinemore.jb",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/cinemore-app-icon.png",
				"backgroundColor": "#F19809"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/cinemore-app-icon.png",
			"manifest": {
				"name": "Cinemore",
				"short_name": "Cinemore",
				"description": "New and trending movies.",
				"start_url": "/",
				"display": "standalone",
				"background_color": "#F19809",
				"theme_color": "#F19809",
				"orientation": "portrait",
				"icons": [
					{
						"src": "./assets/images/cinemore-app-icon.png",
						"sizes": "192x192",
						"type": "image/png",
						"purpose": "any maskable"
					},
					{
						"src": "./assets/images/cinemore-app-icon.png",
						"sizes": "512x512",
						"type": "image/png",
						"purpose": "any maskable"
					}
				]
			}
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/cinemore-splash-icon.png",
					"resizeMode": "contain",
					"backgroundColor": "#F19809",
					"imageWidth": 200
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "33a45ccb-697a-4ea7-9849-829187050aa9"
			}
		}
	}
}
